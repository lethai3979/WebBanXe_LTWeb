@model LTWeb_CodeFirst.Models.Warranty

@{
    ViewData["Title"] = "Details";
}

<h1>Thông tin bảo hành</h1>

<div>
    <h4>Chi tiết</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Mô tả bảo hành
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Content)
        </dd>
        <dt class = "col-sm-2">
            Ẩn
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.IsDeleted)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Chỉnh sửa</a> |
    <a asp-action="Index">Trở lại trang bảo hành</a>
</div>
